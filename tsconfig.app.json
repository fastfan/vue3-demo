{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.ts*", "Element-puls.d.ts", "auto-imports.d.ts"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "moduleResolution": "node", // 使用 Node 的模块解析策略
    "types": ["element-plus/global"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "#/*": ["types/*"]
    }
  }
}
